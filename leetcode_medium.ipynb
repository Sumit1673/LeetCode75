   "source": [
    "from typing import *\n",
    "# Difficulty = MEDIUM\n",
    "def combinationSum4(nums: List[int], target: int) -> int:\n",
    "    dp = {0: 1}\n",
    "    \n",
    "    for total in range(1, target + 1):\n",
    "        dp[total] = 0\n",
    "        for n in nums:\n",
    "            dp[total] += dp.get(total -n, 0)\n",
    "            \n",
    "    return dp[target]"
   ]
  }
